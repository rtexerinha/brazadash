{
  "expo": {
    "name": "BrazaDash",
    "slug": "brazadash",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "brazadash",
    "splash": {
      "backgroundColor": "#1B9B59",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brazadash.app",
      "infoPlist": {
        "NSCameraUsageDescription": "Allow BrazaDash to access your camera for photos.",
        "NSPhotoLibraryUsageDescription": "Allow BrazaDash to access your photos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#1B9B59"
      },
      "package": "com.brazadash.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "brazadash"
            }
          ],
          "category": ["DEFAULT", "BROWSABLE"]
        }
      ]
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#1B9B59"
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "extra": {
      "eas": {
        "projectId": "fc998959-1ecb-47a8-846a-19cd6a95bdfa"
      }
    }
  }
}
